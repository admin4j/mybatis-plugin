<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin4j.framework.mybatis.mapper.SysUserMapper">


    <select id="getOne" resultType="com.admin4j.framework.mybatis.entity.SysUser">
        select *
        from sys_user
        where user_id > 1
           or user_id = "admin" limit
        1
    </select>
</mapper>
